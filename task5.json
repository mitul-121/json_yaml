{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Student Information",
    "type": "object",
    "properties": {
      "student": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "age": {
            "type": "number"
          },
          "grade": {
            "type": "string"
          },
          "subjects": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        },
        "required": [
          "name",
          "age",
          "grade",
          "subjects"
        ]
      }
    },
    "required": [
      "student"
    ]
  }